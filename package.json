    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^5.1.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "typescript": "^4.2.2"
